    <sql>USE ${SF_DATABASE_NAME};
USE SCHEMA ${SF_SCHEMA_NAME};
alter session set timestamp_input_format = 'YYYY-MM-DD HH24:MI:SS.FF1 TZD';

--2009/04/17 00:00:00.000000000

put file://${ETL_FOLDER}/files/avro_${SYSTEM_NAME}_${TABLE_NAME}.avro @${SF_INTERNAL_STAGE} auto_compress=true;


--create additional table with only one variant column
CREATE OR REPLACE TABLE ${SF_SCHEMA_NAME}.${SYSTEM_NAME}_${TABLE_NAME}1_with_AVRO (
 	Avro_File Variant
);

--remove data from the table
--DELETE FROM JA_LOAD.RFX_ITEM_with_Parquet;

--Bulk copy the PDI generated parquet file into this one column field which will create a row 
--for each in the interim table for each row in the parquet file
copy into ${SF_SCHEMA_NAME}.${SYSTEM_NAME}_${TABLE_NAME}1_with_Avro
  from @${SF_INTERNAL_STAGE}/avro_${SYSTEM_NAME}_${TABLE_NAME}.avro
  file_format = (format_name = AVRO_FILE);

--remove data from the target table
DELETE FROM ${SF_SCHEMA_NAME}.${TABLE_NAME};

--insert data in a target table that should be already created using DDL script
INSERT INTO ${SF_SCHEMA_NAME}.${TABLE_NAME} (
     SYSTEM_ID, ${INSERT_COLUMN_LIST}  
) 
    SELECT
         '${SYSTEM_NAME}', ${AVRO_COLUMN_LIST}    
    FROM ${SF_SCHEMA_NAME}.${SYSTEM_NAME}_${TABLE_NAME}1_with_Avro;
   
</sql>
