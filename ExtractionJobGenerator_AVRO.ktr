    <connection>JA_CONN</connection>
    <sql>SELECT '\${Internal.Entry.Current.Directory}' as PH_DIRECTORY_PARAMETER,
	   XMLAGG(XMLELEMENT("entry",
	           XMLFOREST(
				hop_to AS "name",
				NULL AS "description",
				(CASE WHEN hop_to LIKE 'Success%' THEN 'SUCCESS' ELSE 'TRANS' END) AS "type",
				NULL AS "attributes",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'filename' END) AS "specification_method",
				NULL AS "trans_object_id",
				(CASE WHEN hop_to LIKE 'JA Extractio%' THEN '[PLACEHOLDER_DIRECTORY_PARAMETER]/transformation/extraction_' || tbl_name || '.ktr' 
					  WHEN hop_to LIKE 'SF load%' THEN '[PLACEHOLDER_DIRECTORY_PARAMETER]/transformation/load_Snowflake_AVRO.ktr' END) AS "filename",
				NULL AS "transname",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "arg_from_previous",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "params_from_previous",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "exec_per_row",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "clear_rows",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "clear_files",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "set_logfile",
				NULL AS "logfile",
				NULL AS "logext",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "add_date",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "add_time",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'BASIC' END) AS "loglevel",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "cluster",
				NULL AS "slave_server_name",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "set_append_logfile",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'Y' END) AS "wait_until_finished",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "follow_abort_remote",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "create_parent_folder",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "logging_remote_work",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'Pentaho local' END) AS "run_configuration",
				(CASE WHEN hop_to NOT LIKE 'Success%' THEN 'N' END) AS "suppress_result_data",
				(CASE WHEN hop_to LIKE 'JA Extraction%' THEN XMLFOREST('Y' AS "pass_all_parameters") 
					  WHEN hop_to LIKE 'SF load%' THEN XMLFOREST('Y' AS "pass_all_parameters", 
					  												   XMLFOREST('TABLE_NAME' AS "name",
					  												   			 NULL AS "stream_name",
					  												   			 tbl_name AS "value") AS "parameter",
					  												   XMLFOREST('INSERT_COLUMN_LIST' AS "name",
					  												   			 NULL AS "stream_name",
					  												   			 slct_col_list AS "value") AS "parameter",
					  												   	XMLFOREST('AVRO_COLUMN_LIST' AS "name",
					  												   			 NULL AS "stream_name",
					  												   			 prq_col_list AS "value") AS "parameter") 
			     END) AS "parameters",
				--'test' AS "parameters",
				--(CASE WHEN hop_from = 'Start' THEN 'Y' ELSE 'N' END) AS "unconditional"
				'N' AS "parallel",
				'Y' AS "draw",
				'0' AS "nr",
				(CASE WHEN hop_to LIKE 'Success%' THEN '1064'
					  WHEN hop_to LIKE 'SF load%' THEN '764'
					  WHEN hop_to LIKE 'JA Extraction%' THEN '364'
					  ELSE '0' END) AS "xloc",
				(LVL-1) * 100 + 64 AS "yloc",
				NULL AS "attributes_kjc"
	          ))).getClobVal() AS entries,
	   XMLELEMENT("hops", XMLAGG(XMLELEMENT("hop",
								           XMLFOREST(
								             hop_from AS "from",
								             hop_to AS "to",
								             0 AS "from_nr",
								             0 AS "to_nr",
								             'Y' AS "enabled",
								             'Y' AS "evaluation",
								             (CASE WHEN hop_from = 'Start' THEN 'Y' ELSE 'N' END) AS "unconditional"
								          )))).getClobVal() AS hops
FROM (SELECT TABLE_NAME mv_name, NUM_ROWS, SUBSTR(TABLE_NAME, 4) tbl_name,
			 (CASE hop_from_template WHEN 'Start' THEN hop_from_template ELSE hop_from_template || SUBSTR(TABLE_NAME, 4) END) hop_from, 
			 DENSE_RANK () OVER (ORDER BY TABLE_NAME) lvl, 
			 hop_to_template || SUBSTR(TABLE_NAME, 4) hop_to
		FROM ALL_TABLES, (SELECT 'Start' hop_from_template, 'JA Extraction - ' hop_to_template FROM DUAL
						   		UNION SELECT 'JA Extraction - ' hop_from, 'SF load - ' hop_to FROM DUAL
						   		UNION SELECT 'SF load - ' hop_from, 'Success - ' hop_to FROM DUAL)
		WHERE TABLE_NAME LIKE 'MV_%' and table_name = 'MV_AUCTION_BIDS_VALUES'
		ORDER BY TABLE_NAME) tbl
		INNER JOIN (SELECT table_name, 
						   LISTAGG('avro_file:' || COLUMN_NAME,  ', ') WITHIN GROUP (ORDER BY COLUMN_NAME) prq_col_list,
						   LISTAGG(COLUMN_NAME,  ', ') WITHIN GROUP (ORDER BY COLUMN_NAME) slct_col_list
					FROM ALL_TAB_COLUMNS atc 
					WHERE atc.TABLE_NAME like 'MV_%'
					GROUP BY table_name) col ON col.table_name = tbl.mv_name
--WHERE LVL &lt; 2</sql>
